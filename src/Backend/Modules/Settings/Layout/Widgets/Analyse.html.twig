{% if warnings %}
  <div id="widgetSettingsAnalyse" class="card card-default">
    <div class="card-heading">
      <h2 class="card-title">{{ 'lbl.Analysis'|trans|ucfirst }}</h2>
    </div>
    <div class="card-body">
      <ul>
        {% for warning in warnings %}
          <li>{{ warning.message | raw }}</li>
        {% endfor %}
      </ul>
    </div>
  </div>
{% endif %}
